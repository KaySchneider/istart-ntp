{
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "version": "0.0.1.1",
    "manifest_version": 2,
    "default_locale": "en_US",
    "offline_enabled": true,
    "optional_permissions": ["topSites"],
    "permissions": [
        "history","management","storage","unlimitedStorage","\u003Call_urls\u003E", "webNavigation", "tabs"
    ],
    "icons": {
        "128": "istartIcon.png",
        "16":"istartIcon16.png"
    },
    "background": {
        "scripts": ["app/eventPage.js" ],
        "persistent": false
    },
    "chrome_url_overrides" : {
        "newtab": "html/metro.html"
    },
    "web_accessible_resources": [
        "html/metro.html"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com   https://pagead2.googlesyndication.com; object-src 'self'"
}
